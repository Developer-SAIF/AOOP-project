<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="studentVBox"
      alignment="CENTER"
      spacing="10.0"
      xmlns="http://javafx.com/javafx/23.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      stylesheets="/css/Student.css">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20"/>
    </padding>
    <Button fx:id="notificationsButton" text="Notifications"/>
    <Button fx:id="notesButton" text="Notes"/>
    <Button fx:id="timerButton" text="Timer"/>
    <Button fx:id="doubtsolvingButton" text="Doubt Solving"/>
</VBox>
